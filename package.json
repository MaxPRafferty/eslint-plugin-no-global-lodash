{
  "name": "eslint-plugin-no-global-lodash",
  "version": "1.0.2",
  "description": "Lodash should not be loaded globally. If a lodash method is needed only the package for that method should be loaded in the file.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Adalberto Teixeira",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha tests --recursive",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "~3.9.1",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.3.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adalbertoteixeira/eslint-plugin-no-global-lodash.git"
  },
  "bugs": {
    "url": "https://github.com/adalbertoteixeira/eslint-plugin-no-global-lodash/issues"
  },
  "homepage": "https://github.com/adalbertoteixeira/eslint-plugin-no-global-lodash#readme"
}
